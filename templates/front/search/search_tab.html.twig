{% for item in result %}
    <div class="card border border-secondary">
        <h5 class="card-header">{{ item.title }}</h5>
        <div class="card-body">
            <p class="card-text">{{ item.getPresentText()|raw }}</p>
            <a href="{{ path(route, {'id': item.id}) }}" class="btn btn-primary">Посмотреть</a>
        </div>
    </div>
{% endfor %}
