{% if breadCrumbs is defined %}
        <ul class="u-list-inline">
    {% for bc in breadCrumbs %}
        <li class="list-inline-item g-mr-5 {% if loop.last == false %}g-color-primary{% endif %}">
            <a class="u-link-v5 g-color-main" href="{{ bc.path }}">{{ bc.title }}</a>
            {% if loop.last == false %}
                <i class="fa fa-angle-right g-ml-7"></i>
            {% endif %}
        </li>
        {% endfor %}
        </ul>
{% endif %}
